<p id="notice"><%= notice %></p>

<h1>Listing Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Cpf cnpj</th>
      <th>Rua</th>
      <th>Numero</th>
      <th>Cep</th>
      <th>Bairro</th>
      <th>Telefone</th>
      <th>Celular</th>
      <th>Telefone comercial</th>
      <th>Data nascimento</th>
      <th>Rg</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clientes.each do |cliente| %>
      <tr>
        <td><%= cliente.nome %></td>
        <td><%= cliente.cpf_cnpj %></td>
        <td><%= cliente.rua %></td>
        <td><%= cliente.numero %></td>
        <td><%= cliente.cep %></td>
        <td><%= cliente.bairro %></td>
        <td><%= cliente.telefone %></td>
        <td><%= cliente.celular %></td>
        <td><%= cliente.telefone_comercial %></td>
        <td><%= cliente.data_nascimento %></td>
        <td><%= cliente.rg %></td>
        <td><%= link_to 'Show', cliente %></td>
        <td><%= link_to 'Edit', edit_cliente_path(cliente) %></td>
        <td><%= link_to 'Destroy', cliente, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cliente', new_cliente_path %>
